{
  "name": "nzta/silverstripe-okta",
  "description": "A module for okta integration",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/NZTransportAgency/okta",
  "keywords": [
    "silverstripe",
    "okta",
    "sso",
    "cms"
  ],
  "license": "BSD-3-Clause",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:micmania1/php-saml.git"
    }
  ],
  "support": {
    "issues": "https://github.com/NZTransportAgency/okta/issues"
  },
  "require": {
    "silverstripe/cms": "^4.0",
    "silverstripe/framework": "^4.0",
    "symbiote/silverstripe-queuedjobs": "^4.2",
    "guzzlehttp/guzzle": "^6.3",
    "onelogin/php-saml": "dev-sign-logout-request"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
